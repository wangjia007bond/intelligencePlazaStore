<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" 
		controllerName="uplaza.floorMenu" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="floorMenu" showHeader="false" enableScrolling="false" showFooter="false">
		<content>
			<core:Icon class="appBack" src="sap-icon://nav-back" size="1.3rem" color="rgb(155,155,155)" press="onBack"/>
			<Text class="appFloor" text="Chamtime - F1" />
	 		<Image class="appFooter" src="image/Rectangle 18@2x.png" width="375px" height="52px" densityAware="false"/>
			<Image class="appLogo" src="image/U Plaza@2x.png" width="67px" height="16px" densityAware="false" />
			<Image id="map_st" class="appStore" src="image/store_b@2x.png" width="28px" height="31px" densityAware="false"/>
			<Image id="map_game" class="appGame" src="image/game_g@2x.png" width="30px" height="31px" densityAware="false" press="onGame"/>
			<List >
				<StandardListItem></StandardListItem>
				<StandardListItem title="F4"></StandardListItem>
				<StandardListItem title="F3"></StandardListItem>
				<StandardListItem title="F2"></StandardListItem>
				<StandardListItem title="F1" class="selectedColor"></StandardListItem>
				<StandardListItem title="B1"></StandardListItem>
			</List>
		</content>
	</Page>
</core:View>