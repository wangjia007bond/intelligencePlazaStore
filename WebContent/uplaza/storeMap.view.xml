<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="uplaza.storeMap" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page id="storeMap" showHeader="false" enableScrolling="false" showFooter="false">
		<content>
		<Image src="image/02map_bg.png" mode="Background" width="375px" height="627px" densityAware="false" />
	 	<Image class="appHeader" src="image/05 game_header.png" densityAware="false" />   
	 	<core:Icon class="appBack" src="sap-icon://menu2" size="1.3rem" color="rgb(155,155,155)" press="onMenu"/>
	 	<core:Icon class="appCart" src="sap-icon://person-placeholder" size="1.3rem" color="rgb(155,155,155)" press="onCenter"/>
	 	<Text class="appFloor" text="Chamtime - F1" />
	 	<Image class="appFooter" src="image/Rectangle 18@2x.png" width="375px" height="52px" densityAware="false"/>
		<Image class="appLogo" src="image/U Plaza@2x.png" width="67px" height="16px" densityAware="false" />
		<Image id="map_st" class="appStore" src="image/store_b@2x.png" width="28px" height="31px" densityAware="false"/>
		<Image id="map_game" class="appGame" src="image/game_g@2x.png" width="30px" height="31px" densityAware="false" press="onGame"/>
		
		<Image id="map_hm" class="mapHM" src="image/map_hm.png" press="onStore"  densityAware="false"/>
		<Image id="dotyellow" class="mapYellow" src="image/02map_dotyellow.png"  densityAware="false"/>
		<Image id="dotred1" class="mapRed1" src="image/02map_dotred.png"  densityAware="false"/>
		<Image id="dotred2" class="mapRed2" src="image/02map_dotred.png"  densityAware="false"/>
		<Image id="dotred3" class="mapRed3" src="image/02map_dotred.png"  densityAware="false"/>
		<Image id="dotred4" class="mapRed4" src="image/02map_dotred.png" press="onYellow"  densityAware="false"/>
		<Image id="dotpurple1" class="mapPurple1" src="image/02map_dotpurple.png"  densityAware="false"/>
		<Image id="dotpurple2" class="mapPurple2" src="image/02map_dotpurple.png"  densityAware="false"/>
		<Image id="dotpink1" class="mapPink1" src="image/02map_dotpink.png"  densityAware="false"/>
		<Image id="dotpink2" class="mapPink2" src="image/02map_dotpink.png"  densityAware="false"/>
		<Image id="dotgrey1" class="mapGrey1" src="image/02map_dotgrey.png"  densityAware="false"/>
		<Image id="dotgrey2" class="mapGrey2" src="image/02map_dotgrey.png"  densityAware="false"/>
		
		</content>
	</Page>
</core:View>